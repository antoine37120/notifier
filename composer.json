{
    "name": "antoine37120/notifier",
    "description": "Notifier plugin for CakePHP fork",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "antoine37120\\Notifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "antoine37120\\Notifier\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
